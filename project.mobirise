        "primaryColor": "#FFCA3A",
        "secondaryColor": "#6A4C93",
        "successColor": "#2ED889",
        "infoColor": "#14CDF2",
        "warningColor": "#FFC107",
        "dangerColor": "#DA2C3C",
        "mainFont": "Manrope",
        "display1Font": "Manrope",
        "display1Size": 5,
        "display2Font": "Manrope",
        "display2Size": 4,
        "display5Font": "Manrope",
        "display5Size": 2.5,
